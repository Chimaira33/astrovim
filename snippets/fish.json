{
  "sb": {
    "prefix": "sb",
    "body": [
      "#!/data/data/com.termux/files/usr/bin/env fish"
    ]
  },
  "function": {
    "prefix": "fun",
    "body": [
      "function ${1:?}\nend"
    ]
  },
  "wrap": {
    "prefix": "wrap",
    "body": [
      "-w '${1:?}'"
    ]
  },
  "switch": {
    "prefix": "swi",
    "body": "switch \"\\$${1:argv[1]}\"\ncase a\ncase b\ncase c\nend\n"
  },
  "export": {
    "prefix": "ex",
    "body": "set -gx ${1:?} \"\""
  },
  "if": {
    "prefix": "if",
    "body": "if test ${1:?}\n  ${0}\nend"
  },
  "if_string": {
    "prefix": "if_string",
    "body": "if string ${1:?}\n  ${0}\nend"
  },
  "if_set": {
    "prefix": "if_set",
    "body": "if set -q ${1:?}\n  ${0}\nend"
  },
  "argparse": {
    "prefix": "argparse",
    "body": "argparse -isn ${1:name} a b -- \\$argv; or exit\n\nif set -q _flag_a\nend\n\nif set -q _flag_b\nend"
  },
  "getopts": {
    "prefix": "getopts",
    "body": "getopts \\$argv | while read -l key value\n  switch \"\\$key\"\n  case ${1:a}\n  \n  case b\n  \n  case '*'\n \nend\nend\n\n"
  },
  "gchoose": {
    "prefix": "gch",
    "body": "set -l choice (gch {a,b,c})\n\nswitch \"\\$choice\"\ncase a\ncase b\ncase c\nend"
  },
  "gconfirm": {
    "prefix": "gcon",
    "body": "gcon \"${1:?}\" Yes No\nor return\n\n"
  },
  "for": {
    "prefix": "for",
    "body": "for i in ${1:?}\n  ${2:arguments} \"\\$i\"\nend\n\n"
  },
  "null1": {
    "prefix": "null1",
    "body": "1>/dev/null"
  },
  "null2": {
    "prefix": "null2",
    "body": "2>/dev/null"
  },
  "null&": {
    "prefix": "null&",
    "body": "&>/dev/null"
  },
  "fish_trace": {
    "prefix": "fish_trace",
    "body": "set -l fish_trace 1"
  },
  "argv": {
    "prefix": "argv",
    "body": "\\$argv"
  },
  "argv_index": {
    "prefix": "argv_index",
    "body": "\\$argv[${1:1}]"
  },
  "return": {
    "prefix": "return",
    "body": "return"
  }
}
